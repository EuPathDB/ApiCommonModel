#!/usr/bin/perl

use strict;
use lib $ENV{GUS_HOME} . "/lib/perl";
use JSON;
use ApiCommonModel::Model::JbrowseRnaSeqJunctionTracks;

my ($organismAbbrev, $projectName, $isApollo, $applicationType) = @ARGV;

my $result = {"tracks" => [] };

ApiCommonModel::Model::JbrowseRnaSeqJunctionTracks::processOrganism($organismAbbrev, $projectName, $isApollo, $applicationType, $result);

print encode_json($result);
