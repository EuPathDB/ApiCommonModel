#!/usr/bin/perl

use strict;
use ApiCommonModel::Model::JbrowseRnaAndChipSeqTracks;

use Data::Dumper;

use Encode;
use JSON qw(decode_json);
 
my ($orgAbbrev, $projectName, $buildNumber, $webservicesDir, $technologyType, $applicationType) = @ARGV;

my $result = {"tracks" => [] };

JbrowseRnaAndChipSeq::processOrganism($organismAbbrev, $projectName, $buildNumber, $webServicesDir, 'RNASeq', $result);

print encode_json($result);

1;

